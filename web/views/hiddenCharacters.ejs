<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <title>Knaji</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" media="screen" href="/main.css" />
</head>

<body>
    <% hiddenCharacters.forEach((character) => { %>
        <a href="#" id="<%= character %>" class="char" onclick="unhideCharacter('<%= character %>'); return false">
            <%= character %>
        </a>
    <% }); %>

    <div id="loader" class="overlay">
        <div id="loaderText">実行中</div>
    </div>

    <input type="hidden" id="user" value="<%=userName%>" />
    <script
        src="https://code.jquery.com/jquery-3.3.1.min.js"
        integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8="
        crossorigin="anonymous"></script>
    <script src="/hiddenCharacters.js"></script>
</body>
</html>